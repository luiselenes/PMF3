<p id="notice"><%= notice %></p>

<h1>Agricultural Companies</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Phone</th>
      <th>Responsable</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @agricultural_companies.each do |agricultural_company| %>
      <tr>
        <td><%= agricultural_company.name %></td>
        <td><%= agricultural_company.address %></td>
        <td><%= agricultural_company.phone %></td>
        <td><%= agricultural_company.responsable %></td>
        <td><%= link_to 'Show', agricultural_company %></td>
        <td><%= link_to 'Edit', edit_agricultural_company_path(agricultural_company) %></td>
        <td><%= link_to 'Destroy', agricultural_company, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Agricultural Company', new_agricultural_company_path %>